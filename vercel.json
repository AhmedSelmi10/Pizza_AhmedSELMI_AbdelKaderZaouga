{
  "version": 2,
  "builds": [
    { "src": "app.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/articles/:nom", "dest": "/app.js" },
    { "src": "/api/news", "dest": "/app.js" },
    { "src": "/users/(.*)", "dest": "/app.js" },
    { "src": "/auth/(.*)", "dest": "/app.js" },
    { "src": "/note/(.*)", "dest": "/app.js" },

    {
      "src": "/(.*)",
      "dest": "/app.js",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "headers": {
        "Access-Control-Allow-Origin": "https://vetsproject.vercel.app",

        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE",
        "Access-Control-Allow-Headers": "Content-Type, Authorization"
      }
    }
  ]
}
